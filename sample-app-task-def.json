{
    "family": "ecs-dev",
    "taskRoleArn": "arn:aws:iam::232122710061:role/ecs-dev",
    "executionRoleArn": "arn:aws:iam::232122710061:role/ecs-dev-ecs",
    "networkMode": "awsvpc",
    "containerDefinitions": [
      {
        "name": "app",
        "image": "quay.io/turner/turner-defaultbackend:0.2.0",
        "essential": true,
        "portMappings": [
          {
            "containerPort": 8001,
            "hostPort": 8001,
            "protocol": "tcp"
          }
        ],
        "environment": [
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/fargate/service/ecs-dev",
            "awslogs-region": "us-east-2",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ],
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
  }